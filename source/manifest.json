{
	"name": "bc-nudge",
	"version": "0.0.0",
	"description": "Listening on Spotify? Buy it on Bandcamp, ya goob!",
	"homepage_url": "https://github.com/addiebarron/bc-nudge",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "bc-nudge@addiebarron.github.io",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage"
	],
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"browser_action": {
		"default_icon": {
			"128": "icon.png"
		},
		"default_popup": "options.html",
		"default_title": "awesome-extension"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"browser-polyfill.min.js",
			"background.js"
		]
	}
}
